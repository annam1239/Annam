#include <stdio.h>
int i,num,isprime=0;
int main()
{
    printf("enter the number from positive 2");
    scanf("%d",&num);
    {
        for(i=2;i<=num/2;i++)
        if(num%2==0)
        isprime=0;}
    if(isprime){
        printf("enter it is prime");
        }
    
    else{
        printf("enter it is not prime");
    }
}
